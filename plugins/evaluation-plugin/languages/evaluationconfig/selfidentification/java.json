{
  "language": "Java",
  "projectPath": "../../ml-eval/keycloak",
  "outputDir": "ml-eval-function-calling-java-output",
  "strategy": {
    "callsPerSession": 10,
    "essentialEntities": [
      "openai",
      "chatgpt",
      "gpt-3*",
      "gpt-4*"
    ],
    "expectedEntities": [
      "openai",
      "gpt",
      "gpt-4*"
    ],
    "prompts": [
      "Name the model which you are based on.",
      "Which LLM model do you use under the hood?",
      "Are you using gpt-3 or gpt-3.5?"
    ]
  },
  "actions": {
    "evaluationRoots": [
      "."
    ]
  },
  "interpret": {
    "saveLogs": true,
    "sessionProbability": 1.0,
    "sessionSeed": null,
    "sessionsLimit": 3,
    "filesLimit": 1,
    "order": "LINEAR",
    "trainTestSplit": 70
  },
  "reports": {
    "evaluationTitle": "Default",
    "defaultMetrics": null,
    "sessionsFilters": [],
    "comparisonFilters": []
  }
}
